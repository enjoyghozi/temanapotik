@model IEnumerable<Teman_ApotikProj.Models.JenisObat>

@{
    ViewBag.Title = "Index";
}

<h2>Buat Baru</h2>

<p class="navbar-right">
    @Html.ActionLink("Buat Baru", "Create")
</p>



<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Jenis Obat")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Jenis_Obat)
            </td>
            <td>
                @Html.ActionLink("Ubah", "Edit", new { id = item.Id_Jenis_Obat }) |
                @Html.ActionLink("Detail", "Details", new { id = item.Id_Jenis_Obat }) |
                @Html.ActionLink("Hapus", "Delete", new { id = item.Id_Jenis_Obat })
            </td>
        </tr>
    }

</table>
